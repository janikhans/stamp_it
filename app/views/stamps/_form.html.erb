<%= simple_form_for(@stamp) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :speaker %>
    <%= f.input :spoken_date %>
    <%= f.input :completion_date %>
    <%= f.input :proof %>
    <%= f.input :quote %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
